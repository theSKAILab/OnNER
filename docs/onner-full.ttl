@prefix : <http://purl.org/spatialai/onner/onner-full#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/spatialai/onner/onner-full#> .

<http://purl.org/spatialai/onner/onner-full> rdf:type owl:Ontology ;
                                              owl:versionIRI <http://purl.org/spatialai/onner/v1/onner-full> ;
                                              <http://purl.org/dc/terms#bibliographicCitation> "Umayer Reza, Xuelian Zhang, Torsten Hahmann. (2024). OnNER: An Ontology for Semantic Representation of Named Entities in Scholarly Publications." ;
                                              <http://purl.org/dc/terms#created> "2023-02-05"^^xsd:date ;
                                              <http://purl.org/dc/terms#creator> "Torsten Hahmann <http://orcid.org/0000-0002-5331-5052>"@en ,
                                                                                 "Umayer Reza <https://orcid.org/0000-0003-4013-3513>"@en ,
                                                                                 "Xuelian Zhang <https://orcid.org/0000-0002-0708-4070>"@en ;
                                              <http://purl.org/dc/terms#description> "OnNER is designed to capture and represent named entities — terms identified and labeled using named entity recognition (NER) methods — along with relevant semantic context, such as their location within a document, from scholarly publications. The ontology is aligned with Basic Formal Ontology (BFO) and Information Artifact Ontology (IAO), and it also incorporates relevant high-level concepts from Relation Ontology (RO), Ontology for Biomedical Investigations (OBI), and NCBI organismal classification (NCBITaxon)."@en ;
                                              <http://purl.org/dc/terms#identifier> <http://purl.org/spatialai/onner/onner-full> ;
                                              <http://purl.org/dc/terms#issued> "2024-05-24"^^xsd:date ;
                                              <http://purl.org/dc/terms#license> <https://creativecommons.org/licenses/by/4.0/> ;
                                              <http://purl.org/dc/terms#modified> "2024-05-30"^^xsd:date ;
                                              <http://purl.org/dc/terms#publisher> "Spatial Knowledge and Artificial Intelligence (SKAI) Lab, University of Maine"@en ;
                                              <http://purl.org/dc/terms#source> <http://example.com/source> ;
                                              <http://purl.org/dc/terms#subject> "Information Extraction" ,
                                                                                 "Named Entity Representation" ,
                                                                                 "Scholarly Publication" ;
                                              <http://purl.org/dc/terms#title> "OnNER: An Ontology for Semantic Representation of Named Entities in Scholarly Publications"@en ;
                                              <http://purl.org/ontology/bibo#doi> "Digital Object Identifier" ;
                                              <http://purl.org/ontology/bibo#status> "stable" ;
                                              owl:versionInfo "1.0"@en ;
                                              <http://xmlns.com/foaf/0.1#logo> <http://example.com/ontology_logo.png> .

