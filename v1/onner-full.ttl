@prefix deo: <http://purl.org/spar/deo#> .
@prefix iao: <http://purl.obolibrary.org/obo/IAO_#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix biro: <http://purl.org/spar/biro#> .
@prefix cito: <http://purl.org/spar/cito#> .
@prefix doco: <http://purl.org/spar/doco#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fabio: <http://purl.org/spar/fabio#> .
@prefix onner: <http://purl.org/spatialai/onner/onner-full#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://purl.org/spatialai/onner/onner-full#> .

<http://purl.org/spatialai/onner/onner-full> rdf:type owl:Ontology ;
                                              owl:versionIRI <http://purl.org/spatialai/onner/v1/onner-full> ;
                                              dcterms:bibliographicCitation "Umayer Reza, Xuelian Zhang, Torsten Hahmann. (2024). OnNER: An Ontology for Semantic Representation of Named Entities in Scholarly Publications." ;
                                              dcterms:created "2023-02-05"^^xsd:date ;
                                              dcterms:creator "Torsten Hahmann <http://orcid.org/0000-0002-5331-5052>"@en ,
                                                              "Umayer Reza <https://orcid.org/0000-0003-4013-3513>"@en ,
                                                              "Xuelian Zhang <https://orcid.org/0000-0002-0708-4070>"@en ;
                                              dcterms:description "OnNER is designed to capture and represent named entities — terms identified and labeled using named entity recognition (NER) methods — along with relevant semantic context, such as their location within a document, from scholarly publications. The ontology is aligned with Basic Formal Ontology (BFO) and Information Artifact Ontology (IAO), and it also incorporates relevant high-level concepts from Relation Ontology (RO), Ontology for Biomedical Investigations (OBI), and NCBI organismal classification (NCBITaxon)."@en ;
                                              dcterms:identifier <http://purl.org/spatialai/onner/onner-full> ;
                                              dcterms:issued "2024-05-24"^^xsd:date ;
                                              dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
                                              dcterms:modified "2024-05-30"^^xsd:date ;
                                              dcterms:publisher "Spatial Knowledge and Artificial Intelligence (SKAI) Lab, University of Maine"@en ;
                                              dcterms:source <http://example.com/source> ;
                                              dcterms:subject "Information Extraction" ,
                                                              "Named Entity Representation" ,
                                                              "Scholarly Publication" ;
                                              dcterms:title "OnNER: An Ontology for Semantic Representation of Named Entities in Scholarly Publications"@en ;
                                              bibo:doi "Digital Object Identifier" ;
                                              bibo:status "stable" ;
                                              owl:versionInfo "1.0"@en ;
                                              foaf:logo <http://example.com/ontology_logo.png> .

