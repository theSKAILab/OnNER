#################################################################

###  http://purl.org/spatialai/onner/onner-full#captionText
onner:captionText rdf:type owl:DatatypeProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( onner:Figure
                                              onner:Listing
                                              onner:Table
                                            )
                              ] ;
                  rdfs:range xsd:string ;
                  rdfs:comment "A text segment containing the description of a floating document element (e.g., a figure, a table, or a listing)."@en ;
                  rdfs:label "caption text"@en .


###  http://purl.org/spatialai/onner/onner-full#doi
onner:doi rdf:type owl:DatatypeProperty ;
          rdfs:domain onner:ScholarlyPublication ;
          rdfs:range xsd:string ;
          rdfs:comment "A text segment containing a unique digital identifier assigned to a scholarly publication released online."@en ;
          rdfs:label "doi"@en .


###  http://purl.org/spatialai/onner/onner-full#figureNumber
onner:figureNumber rdf:type owl:DatatypeProperty ;
                   rdfs:domain onner:Figure ;
                   rdfs:range xsd:nonNegativeInteger ;
                   rdfs:comment "A unique figure number within a document."@en ;
                   rdfs:label "figure number"@en .


###  http://purl.org/spatialai/onner/onner-full#keyword
onner:keyword rdf:type owl:DatatypeProperty ;
              rdfs:domain onner:Keywords ;
              rdfs:range xsd:string ;
              rdfs:comment "A text segment containing a keyword mentioned in a scholarly publication."@en ;
              rdfs:label "keyword"@en .


###  http://purl.org/spatialai/onner/onner-full#labelText
onner:labelText rdf:type owl:DatatypeProperty ;
                rdfs:domain onner:Label ;
                rdfs:range xsd:string ;
                rdfs:comment "A text segment containing a label assigned to a labeled term (also known as a named entity)."@en ;
                rdfs:label "label text"@en .


###  http://purl.org/spatialai/onner/onner-full#labeledTermText
onner:labeledTermText rdf:type owl:DatatypeProperty ;
                      rdfs:domain onner:LabeledTerm ;
                      rdfs:range xsd:string ;
                      rdfs:comment "A text segment containing a labeled term (also known as a named entity) identified within a text fragment."@en ;
                      rdfs:label "labeled term text"@en .


###  http://purl.org/spatialai/onner/onner-full#length
onner:length rdf:type owl:DatatypeProperty ;
             rdfs:domain onner:LabeledTerm ;
             rdfs:range xsd:nonNegativeInteger ;
             rdfs:comment "The total number of characters in a labeled term (also known as a named entity)."@en ;
             rdfs:label "length"@en .


###  http://purl.org/spatialai/onner/onner-full#listingNumber
onner:listingNumber rdf:type owl:DatatypeProperty ;
                    rdfs:domain onner:Listing ;
                    rdfs:range xsd:nonNegativeInteger ;
                    rdfs:comment "A unique listing number within a document."@en ;
                    rdfs:label "listing number"@en .


###  http://purl.org/spatialai/onner/onner-full#offset
onner:offset rdf:type owl:DatatypeProperty ;
             rdfs:domain onner:LabeledTerm ;
             rdfs:range xsd:nonNegativeInteger ;
             rdfs:comment "The starting position of a labeled term (also known as a named entity) within a paragraph."@en ;
             rdfs:label "offset"@en .


###  http://purl.org/spatialai/onner/onner-full#organizationAddress
onner:organizationAddress rdf:type owl:DatatypeProperty ;
                          rdfs:domain onner:Organization ;
                          rdfs:range xsd:string ;
                          rdfs:comment "A text segment containing an organization's address."@en ;
                          rdfs:label "organization address"@en .


###  http://purl.org/spatialai/onner/onner-full#organizationName
onner:organizationName rdf:type owl:DatatypeProperty ;
                       rdfs:domain onner:Organization ;
                       rdfs:range xsd:string ;
                       rdfs:comment "A text segment containing an organization's name."@en ;
                       rdfs:label "organization name"@en .


###  http://purl.org/spatialai/onner/onner-full#paragraphText
onner:paragraphText rdf:type owl:DatatypeProperty ;
                    rdfs:domain onner:Paragraph ;
                    rdfs:range xsd:string ;
                    rdfs:comment "A text segment containing a paragraph."@en ;
                    rdfs:label "paragraph text"@en .


###  http://purl.org/spatialai/onner/onner-full#personName
onner:personName rdf:type owl:DatatypeProperty ;
                 rdfs:domain onner:Person ;
                 rdfs:range xsd:string ;
                 rdfs:comment "A text segment containing a person's name."@en ;
                 rdfs:label "person name"@en .


###  http://purl.org/spatialai/onner/onner-full#positionInParentDocumentPart
onner:positionInParentDocumentPart rdf:type owl:DatatypeProperty ;
                                   rdfs:domain onner:Paragraph ;
                                   rdfs:range xsd:nonNegativeInteger ;
                                   rdfs:comment "A unique number indicating the hierarchical position of a document part within its parent document part."@en ;
                                   rdfs:label "position in parent document part"@en .


###  http://purl.org/spatialai/onner/onner-full#publicationDate
onner:publicationDate rdf:type owl:DatatypeProperty ;
                      rdfs:domain onner:ScholarlyPublication ;
                      rdfs:range xsd:dateTime ;
                      rdfs:comment "A date when a publication is made publicly available or officially released to the audience."@en ;
                      rdfs:label "publication date"@en .


###  http://purl.org/spatialai/onner/onner-full#publicationTitle
onner:publicationTitle rdf:type owl:DatatypeProperty ;
                       rdfs:domain onner:ScholarlyPublication ;
                       rdfs:range xsd:string ;
                       rdfs:comment "A text segment containing the title of a scholarly publication."@en ;
                       rdfs:label "publication title"@en .


###  http://purl.org/spatialai/onner/onner-full#publicationVenue
onner:publicationVenue rdf:type owl:DatatypeProperty ;
                       rdfs:domain onner:ScholarlyPublication ;
                       rdfs:range xsd:string ;
                       rdfs:comment "A text segment indicating the venue of the conference where a scholarly publication is presented."@en ;
                       rdfs:label "publication venue"@en .


###  http://purl.org/spatialai/onner/onner-full#schemaName
onner:schemaName rdf:type owl:DatatypeProperty ;
                 rdfs:domain onner:LabelingSchema ;
                 rdfs:range xsd:string ;
                 rdfs:comment "A text segment containing a schema name."@en ;
                 rdfs:label "schema name"@en .


###  http://purl.org/spatialai/onner/onner-full#sectionNumber
onner:sectionNumber rdf:type owl:DatatypeProperty ;
                    rdfs:domain onner:Section ;
                    rdfs:range xsd:string ;
                    rdfs:comment "A text segment containing a section number."@en ;
                    rdfs:label "section number"@en .


###  http://purl.org/spatialai/onner/onner-full#sectionTitle
onner:sectionTitle rdf:type owl:DatatypeProperty ;
                   rdfs:domain onner:Section ;
                   rdfs:range xsd:string ;
                   rdfs:comment "A text segment containing a section title."@en ;
                   rdfs:label "section title"@en .


###  http://purl.org/spatialai/onner/onner-full#statusAssignmentDate
onner:statusAssignmentDate rdf:type owl:DatatypeProperty ;
                           rdfs:domain onner:LabeledTermStatus ;
                           rdfs:range xsd:dateTime ;
                           rdfs:comment "A specific date and time at which a particular status is assigned to a labeled term (also known as a named entity)."@en ;
                           rdfs:label "status assignment date"@en .


###  http://purl.org/spatialai/onner/onner-full#systemVersion
onner:systemVersion rdf:type owl:DatatypeProperty ;
                    rdfs:domain onner:NER_System ;
                    rdfs:range xsd:string ;
                    rdfs:comment "A text segment containing the version number of an NER system."@en ;
                    rdfs:label "system version"@en .


